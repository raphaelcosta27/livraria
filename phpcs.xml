<?xml version="1.0"?>
<ruleset name="Laravel Skeleton">
    <description>PHPCS rules for my Laravel skeleton project</description>
    <exclude-pattern>vendor/*</exclude-pattern>
    <exclude-pattern>node_modules/*</exclude-pattern>
    <exclude-pattern>app/*</exclude-pattern>
    <file>app</file>
    <file>routes</file>
    <file>database</file>
    <file>tests</file>
</ruleset>